<!--javascript to change qpf options -->
<script>
  function imgformatqpf(nam) {
    var prodval;
    var prodnav;
    var prodimg;
    prodval = "validtimes-qpf-" + nam;
    prodnav = "fordays-qpf-" + nam;
    prodimg = "forimg-qpf-" + nam;

    //Change valid times
    document.getElementById("validtimes-qpf-opt1").style.display= "none";
    document.getElementById("validtimes-qpf-opt2").style.display= "none";
    document.getElementById("validtimes-qpf-opt3").style.display= "none";
    document.getElementById("validtimes-qpf-opt4").style.display= "none";
    document.getElementById(prodval).style.display = "block";

    //Change navbar
    document.getElementById("fordays-qpf-opt1").style.display= "none";
    document.getElementById("fordays-qpf-opt2").style.display= "none";
    document.getElementById("fordays-qpf-opt3").style.display= "none";
    document.getElementById("fordays-qpf-opt4").style.display= "none";
    document.getElementById(prodnav).style.display = "block";

    //Change images
    document.getElementById("forimg-qpf-opt1").style.display= "none";
    document.getElementById("forimg-qpf-opt2").style.display= "none";
    document.getElementById("forimg-qpf-opt3").style.display= "none";
    document.getElementById("forimg-qpf-opt4").style.display= "none";
    document.getElementById(prodimg).style.display = "block";
  }

</script>

