<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
<script>

$(document).keydown(
  function(e)
  {
    if (e.keyCode == 39) {
        $(".overviews:focus").closest('li').next().find('a.overviews').focus();
    }
    if (e.keyCode == 37) {
        $(".overviews:focus").closest('li').prev().find('a.overviews').focus();
    }
  }
);

$(function() {
  $('#firsts').focus();
});

</script>


