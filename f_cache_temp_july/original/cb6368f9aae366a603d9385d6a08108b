      <div class="product-content-new" id="ovw">
        <!--'Overview' and Headlines tab -->


<!-- Add a bit of code to allow navigation using arrows between days. -->

<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
<script>

$(document).keydown(
  function(e)
  {
    if (e.keyCode == 39) {
        $(".overviewo:focus").closest('li').next().find('a.overviewo').focus();
    }
    if (e.keyCode == 37) {
        $(".overviewo:focus").closest('li').prev().find('a.overviewo').focus();
    }
  }
);

$(function() {
  $('#firsto').focus();
});

</script>

