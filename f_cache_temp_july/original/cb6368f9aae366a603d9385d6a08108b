<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
<script>

$(document).keydown(
  function(e)
  {
    if (e.keyCode == 39) {
        $(".overviewm:focus").closest('li').next().find('a.overviewm').focus();
    }
    if (e.keyCode == 37) {
        $(".overviewm:focus").closest('li').prev().find('a.overviewm').focus();
    }
  }
);

$(function() {
  $('#firstm').focus();
});

</script>

