<script type="text/javascript">

// Function to create a month "label" to be used on the UI page
function getShortMonth(month)   {
        switch(month)   {
                case 0: return 'jan';
                case 1: return 'feb';
                case 2: return 'mar';
                case 3: return 'apr';
                case 4: return 'may';
                case 5: return 'jun';
                case 6: return 'jul';
                case 7: return 'aug';
                case 8: return 'sep';
                case 9: return 'oct';
                case 10: return 'nov';
                case 11: return 'dec';
                default: return false;
        }
}
// Function to convert JavaScript month number to a 2-digit month label like "01" or "02"
function getMonthNumbers(month)   {
        switch(month)   {
                case 0: return '01';
                case 1: return '02';
                case 2: return '03';
                case 3: return '04';
                case 4: return '05';
                case 5: return '06';
                case 6: return '07';
                case 7: return '08';
                case 8: return '09';
                case 9: return '10';
                case 10: return '11';
                case 11: return '12';
                default: return false;
        }
}

	// JQuery commands to run after the page is ready to display
	$(document).ready(function () {
		
		// get yesterday's date, since that will be the latest climate data we have
		var d = new Date();
 		d.setDate(d.getDate()-1);
 		var latestYear = d.getFullYear();
 		
 		// create image tag using yesterday's date
 		var imageLink = '<img src="/images/mpx/Climate/MSP/KMSP' + latestYear + 'plot.png" alt="graph of temperatures and precipitation this year at MSP airport">';
 		$("#climateImage").html(imageLink); 
 		
 		
 		// create select lists for years using the current year as latest in the list
 		var selectList;
 		for (i=latestYear; i>=1995; i--) {
 			selectList += "<option value=\"" + i + "\">" + i + "</option>"; 
 		} 
 		$("#YR").html(selectList);

 		var selectList = '';
 		for (i=latestYear; i>=2006; i--) {
 			selectList += "<option value=\"" + i + "\">" + i + "</option>"; 
 		} 
 		$("#graphYR").html(selectList);
	

 		var calMonth = getShortMonth(d.getMonth());		
 		var calMonthNumber = getMonthNumbers(d.getMonth());		
 		
 		
 		// in the select lists for the F6, graphs and records, use the date to pre-select the most current values in the list
 		$("#MO").val(calMonth);
 		$("#graphMO").val(calMonthNumber);
// 		$("#recordMO").val(calMonth);
 		$("#YR").val(latestYear);
 		$("#graphYR").val(latestYear);


    // Function to run when the user clicks on the submit button to get an F6 report 
    // Month and Year will be read from the HTML form and used to create URL for PDF version of F6. 
 		$("#getF6").click(function() {
 			var F6Link = "/media/mpx/Climate/MSP/" + $("#MO").val() + $("#YR").val() + ".pdf";
 			window.open(F6Link);
 		});

	// Function to run when a user clicks on the submit button to get a climate graph
 		$("#getGraph").click(function() {
 			var F6Link = "/images/mpx/Climate/MSP/KMSP" + $("#graphYR").val() + $("#graphMO").val() +   "plot-2.png";
 			window.open(F6Link);
 		});
 		
 		
	});
	
	</script></div>
            </div>
            <div style="clear:both"></div>
            
