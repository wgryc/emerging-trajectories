<script>

    var oldJqVers;
    $(window).on("load",function(e) {
        setTimeout(function(e){
            if ($(window).width() >= 992) {
                var hash = window.location.hash;

                $(".nav-tabs li").removeClass("active");
                if(hash != "" )
                {
                    //hash && $('ul.nav a[href="' + hash + '"]').parent().addClass("active");
                    // hash && oldJqVers('ul.nav a[href="' + hash + '"]').tab('show');
                    $('.nav-tabs li a[href="' + hash + '"]').parent().addClass("active");
                    var contId = hash.replace("#","");
                      $(".tab-content").find(".active").removeClass("active");
                    $('.tab-content  div[id="' + contId + '"]').addClass("active");

                }
                else
                {
                    
                    $(".nav-tabs li.scroll_tab_first").addClass("active");

                }

        }
        }, 10);
    });
    $(document).ready(function(e) {

        if (location.hash) {
            setTimeout(function() {

                window.scrollTo(0, 0);
            }, 1);
        }

        if ($(window).width() >= 992) {
            try {
                highlightsCompResize();
            } catch (e) {}

            $(".nav-tabs li").removeClass("active");
            var hash = window.location.hash;
            if (hash === "") {
                var activeItemLS = $(".tab-content").children(".active");
                // var activeItemLS = $(".tab-content").find(".active");
                //activeItemLS.removeClass("active");
                //$(".tab-content").find(".tabcontent0").addClass("active"); 

                if (typeof activeItemLS.attr("id") == "undefined") {
                    // $(".nav-tabs").find(".active").removeClass("active");
                    $(".nav-tabs").find(".tab0").addClass("active");
                    $(".tab-content").find(".tabcontent0").addClass("active");
                } else {
                    // $(".nav-tabs").find(".active").removeClass("active");
                    $(".nav-tabs").find(".tab" + (parseInt(activeItemLS.data("tabid")))).addClass("active");
                }
            } else {


                oldJqVers = $;

                hash && $('ul.nav a[href="' + hash + '"]').tab('show');


            }

        }

        $(window).resize(function() {
            if ($(window).width() < 991) {

                var hash = window.location.hash;
                var oldItem = $(".mobileul").find(".fa-caret-up");
                oldItem.parent().removeClass("active");
                oldItem.removeClass();
                oldItem.addClass("fa fa-caret-down");

                var activeItemRSS = $(".tab-content").find(".active");



                if (typeof activeItemRSS.attr("id") != "undefined") {

                    var oldActiveItem = $(".mobileul").find(".tabmobile" + (parseInt(activeItemRSS.data("tabid"))));
                    oldActiveItem.children("i").removeClass();
                    oldActiveItem.children("i").addClass("fa fa-caret-up white");

                    $(".mobileli").removeClass("active");

                    oldActiveItem.addClass("active");
                }
            }

            if ($(window).width() > 991) {
                var activeItemRLS = $(".tab-content").children(".active");
                //var activeItemRLS =  $(".tab-content .tab-pane.active");
                if (typeof activeItemRLS.attr("id") == "undefined") {
                    $(".nav-tabs").find(".active").removeClass("active");
                    $(".nav-tabs").find(".tab0").addClass("active");
                    $(".tab-content").find(".tabcontent0").addClass("active");
                } else {
                    $(".nav-tabs").find(".active").removeClass("active");
                    $(".nav-tabs").find(".tab" + (parseInt(activeItemRLS.data("tabid")))).addClass("active");
                }
            }
        });


        $('.mobileul').find("li").on('click', function(e) {
            $(this).children("i").removeClass();
            var oldItem = $(".mobileul").find(".fa-caret-up");
            oldItem.removeClass();
            oldItem.addClass("fa fa-caret-down");
            $(this).children("i").addClass("fa fa-caret-up white");
        });

    });


    $(function(e) {
        $(".nav-tabs li").removeClass("active");
        //var hash = window.location.hash;
        var hash = window.location.hash;
        hash && $('ul.nav a[href="' + hash + '"]').tab('show');
        $('.nav-tabs a').click(function(e) {
            $(this).tab('show');
            var scrollmem = $('body').scrollTop();
            window.location.hash = this.hash;
            $('html,body').scrollTop(scrollmem);
        });
    });

    $(function() {


        var oldMob$ =$;
        $(".nav-tabs li").removeClass("active");
        var hash = window.location.hash;
        hash && $('ul.mobileul a[href="' + hash + '"]').tab('show');
        var oldItem = $(".mobileul").find(".fa-caret-up");
        oldItem.removeClass();
        oldItem.addClass("fa fa-caret-down");
        $(".mobileul").find("li.active").children("i").removeClass();
        $(".mobileul").find("li.active").children("i").addClass("fa fa-caret-up white");

        $('.mobileul li').click(function(e) {

            oldMob$(this).find("a").tab('show');
            var scrollmem = $('body').scrollTop();
            window.location.hash = $(this).find("a").get(0).hash;
            $('html,body').scrollTop(scrollmem);
        });
    });

 $(window).on('load', function() {
        var scrollmem = $('body').scrollTop();
        $('html,body').scrollTop(scrollmem);
    });




    setTimeout(function() {

        window.scrollTo(0, 0);
    }, 1);
    //window.scrollTo(0,0);
   $(window).on('load', function() {
        $('[data-toggle="tooltip"]').tooltip();
        $('.text-tooltip').tooltip({
            trigger: 'hover'
        });
    });

    var tabScroll = null;
    $(document).ready(function() {
        tabScroll = $('#c17v4-tabScroll').scrollTabs();
        var url = window.location.pathname;
        var isArabic = (url.indexOf("/ar/") > -1 || url.indexOf("/ar.") > -1);
        if (isArabic)
            $("#c17v4-tabScroll").css("direction", "rtl");

        $('ul#c17v4-tabScroll li').click(function(e) {
            //debugger;


            $('ul#c17v4-tabScroll li').removeClass('active');
            var tab_id = $(this).find("a").attr('href');
            $('.tab-pane').removeClass('active');
            $(this).addClass('active');


            //console.log(dataPath1);
            $(tab_id).addClass('active');


            //var mode = getCookieValueByName('wcmmode');

            if ($(tab_id).find('.multiHighlights_card').length >= 1) {
                $(tab_id).find('.multiHighlights_card').show();
                setTimeout(function() {
                    $(tab_id).find('.multiHighlights_card').find(".hero").find('.slick-prev').trigger('click');
                }, 700);
                $(tab_id).find('.multiHighlights_card').find(".hero").find('.slick-next').trigger('click');
            }


            $(".tabctrl").each(function() {
                //console.log("tabcontrol inside");
                var tabPane = $(this).find(".tab-pane");
                for (var j = 0; j < tabPane.length; j++) {
                    var classVal = $(this).find(".tabcontent" + j).attr('class');


                    if ($(this).find(".tabcontent" + j).find(".multiHighlights_card").length >= 1 && !(classVal.indexOf("active") >= 0)) {
                        //console.log("hiding");
                        $(this).find(".tabcontent" + j).find(".multiHighlights_card").hide();
                    }
                }
            });

            try {
                //resizeMediaGrid();
                highlightsCompResize();
            } catch (ae) {
                //console.log(ae.message);
            }

            if (tab_id != window.location) {
                window.history.pushState({
                    path: tab_id
                }, '', tab_id);
                e.preventDefault();
            }
        });
    });

    $(document).on("ready", function(e) {
        
        

        /* Remove Empty Tab content - For Kendo*/
        var $tabcontent,$tabList,tabContentLength;
        $tabcontent = $('.tabctrl .tab-content .tab-pane');
        $tabList = $('.tabctrl .scroll_tab_inner li');
		tabContentLength = $tabcontent.length;
        console.log(tabContentLength);

        if($tabcontent.find("*").is(".kendo-tab-inline")){
			 for(i = 0; i <tabContentLength; i++)
             {  
                 if($tabcontent.eq(i).find('.kendo-tab-inline ul li').html() == undefined){
                    $tabList.eq(i).css("display","none");
                    $tabcontent.eq(i).find('.kendo-tab-inline ul').css("display","none");
                 }
              }
        }

                    
    });

       $(document).on("ready", function(e) {
            console.log($("#c17v4-tabScroll li"));
           console.log($("#c17v4-tabScroll li:visible"));
            //$("#c17v4-tabScroll li").first().addClass("active");
        });
</script>

