        console.log('null');
        $(".navbar-collapse>.navbar-nav>li.mega-menu").removeClass("active");
        $(".navbar-collapse>.navbar-nav>li.mega-menu").each(function(index, obj) {
            if ($(this).children("a").text().toLowerCase().indexOf("null".toLowerCase()) === 0)
                $(this).children("a").addClass("active");
        });
    });
