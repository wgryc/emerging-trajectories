        function enrich(personId, profileId) {
            console.log('enrich');

            var url = 'https://api.gie.net' + '/profiles/' + profileId + '/personid/' + personId + '/websiteid/' + 7,
                method = 'PATCH';

            $.ajax({
                url: url,
                method: method,
                success: success,
                error: error,
            });

            function success(data) {
                // Update session
                $.ajax({
                    url: '/account/identifyprofile/',
                    method: 'POST',
                    data: data,
                    error: error
                });
            }

            function error(err) {
                console.log(err);
            }
        }
