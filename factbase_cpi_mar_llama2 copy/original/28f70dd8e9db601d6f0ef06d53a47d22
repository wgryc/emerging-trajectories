<script type="text/javascript">


	
var chart = [];



var makeDrillDownChart = function(chartTitle, topCategories, dataArray, chartIndex){



var thisChartTitle = chartTitle;
var thisTopCategories = topCategories;
var thisDataArray = dataArray;
var thisChartIndex = chartIndex;

/*   console.log(thisDataArray);   */

	
function setChart(name, categories, data, extraData1, extraData2, color) {
	
	chart[thisChartIndex].currentExtraData1 = extraData1 || "";
	chart[thisChartIndex].currentExtraData2 = extraData2 || "";
	

		chart[thisChartIndex].xAxis[0].setCategories(categories);

		chart[thisChartIndex].series[0].remove();
		
		
/*   console.log(chart[thisChartIndex].currentExtraData);   */

		chart[thisChartIndex].addSeries({

			name: name,

			data: data,
			
			color: color || 'white'

		});
		
	
/*   	console.log(chart[thisChartIndex]);   */

	}
	
	
	
	chart[thisChartIndex] = new Highcharts.Chart({

		chart: {

			renderTo: 'chartDiv'+thisChartIndex,

			type: 'column',
			margin: [100,50,80,50] 
/*   adjust margin to fit chart objects (top, right, bottom, left)   */

		},



	    title: {

                text: thisChartTitle,
			

                align: 'left',

                margin: 20,

                x: 0,

                style: {

                    color: '#000000',

                    fontFamily: 'sans-serif',

                    fontWeight: 'bold',

                    width: '540px',

                    fontSize: '14px'

                }

            },



            subtitle: {

                text: "Click on columns to drill down",

                align: 'center',

                x: 0,

				y: 40,

                style: {

                    color: '#744a83',

                    fontFamily: 'sans-serif',

                    fontWeight: 'bold',

                    backgroundColor: '#fff',

                    fontSize: '12px'

                }

            },

		xAxis: {

			categories: thisTopCategories,

			lineWidth: 1.2, 

			lineColor: '#808080',
			
			
		
			
            
/*   max: 1, min: 0,    */

			labels: {

				style: {

					color: '#000000', fontFamily: 'Calibri, Verdana, Arial, Helvetica, sans-serif', fontSize: '10.5pt', fontWeight: 'normal'}

					}

		},

		

		yAxis: {
			
/*    tickInterval: 25000,   */

			lineWidth: 1.2, lineColor: '#808080',
			plotLines: [{"value":0,"color":"#c0c0c0","dashStyle":"solid","width":1}],

            labels: {


					formatter: function() {
						return Highcharts.numberFormat(this.value, 1);
					},


				style: {

					color: '#000000', fontWeight: 'normal'}



					}, 
/*   style for y axis numbers   */

            gridLineColor: '#d1d1d1',   
     /*   makes y axis background lines light gray   */

            gridLineWidth: 0.5,  
     /*   size of y axis lines   */

            gridLineDashStyle: 'longdash',  
     /*   dash style for y axis lines   */

                      
     /*    min: 0, max: 6,            */
	 /*  starting value for y axis    */

             title: {

					text: 'Percent',  
     /*   y-axis title   */
					align: 'high', 
					rotation: 0, 
					x: 70,
					y: -20,
					style: {
            			color: 'black', 
						backgroundColor: '#ffffff', 
						fontFamily: 'Calibri, Verdana, Arial, Helvetica, sans-serif', 
						fontSize: '11pt', 
						fontWeight: 'bold'
					 },

                        }

                },

		plotOptions: {

			series: {
				
			 plotLines: [{
                        "value": 0,
                        "color": "#c0c0c0",
                        "dashStyle": "solid",
                        "width": 1
                    }],			
			maxPointWidth: 80,

				cursor: 'pointer',

				 shadow: false, 
     /*   enable for shadows on bars   */

				 pointPadding: 0.1, 
     /*   padding between each column or bar   */

                 groupPadding: 0.1, 
     /*   space between groups   */
				 minPointLength: 3,

				point: {

					events: {

						click: function() {

							var drilldown = this.drilldown;

							if (drilldown) { 
     /*    drill down   */
							
     /*   console.log(drilldown.extraData);   */

								setChart(drilldown.name, drilldown.categories, drilldown.data, drilldown.extraData1, drilldown.extraData2, drilldown.color);

							} else { 
     /*    restore   */
							
							
     /*   	console.log(thisDataArray);   */

								setChart(name, thisTopCategories, thisDataArray);

							}

						}

					}
						

				},
			

				dataLabels: {

					enabled: false,

					color: '#000000',

					style: {

						fontWeight: 'normal'

					},

					formatter: function() {

						return Highcharts.numberFormat(this.y, 1) +"";

					}

				}

			}

		},

		

		

		  legend: {

			
			layout: 'vertical',  
     /*   make vertical or horizontal   */

           	backgroundColor: 'none',

			borderWidth: 0,  
     /*   Use a white border to correct print issue in Chrome   */

           	borderColor:'white',

			shadow: false,

            align: 'left',  
     /*   alignment of legend items   */

            verticalAlign: 'top',

            x: 100, 
     /*   Higher number moves legend right   */

            y: 40, 
     /*   Higher number moves legend down   */

            width: 700, 
     /*   small widths will cause horizontal layouts to stack   */

                        floating: false, 
     /*   true makes legend independent of chart objects   */

                        reversed: false, 
     /*   reverses legend item order - handy to line up with column order   */

                       itemStyle: {  
     /*   Three legend item styles for active, hover, and deselected   */

                    fontFamily: 'Calibri, Verdana, Arial, Helvetica, sans-serif', fontSize: '11pt', fontWeight: 'bold', lineHeight: 1.3, color: '#000000', cursor: "default", textAlign: 'center'

                    

                },

					itemHiddenStyle: {

                   fontFamily: 'Calibri, Verdana, Arial, Helvetica, sans-serif', fontSize: '11pt', fontWeight: 'bold', lineHeight: 1.3, cursor: "default", textAlign: 'center'

                    

                },

					itemHoverStyle: {

                   fontFamily: 'Calibri, Verdana, Arial, Helvetica, sans-serif', fontSize: '11pt', fontWeight: 'bold', lineHeight: 1.3, cursor: "default", textAlign: 'center'

                    

                }

                       },

		
		

		 
     /*   TOOLTIP (making from table)   */

            tooltip: {
			
				shape: 'rect',				
				followPointer: true,
				positioner: function(labelWidth, labelHeight, point) {         
					var xCurPos = point.plotX + 10, yCurPos = point.plotY + 10;
					return {x: xCurPos - (xCurPos + labelWidth >= this.chart.containerWidth ? labelWidth : 0),y: yCurPos - (yCurPos + labelHeight >= this.chart.containerHeight ? labelHeight : 0)};
				},

                backgroundColor: '#FCFFC5',

                useHTML: false,

                style: {

                    color: '#000000',

                    fontFamily: 'Calibri, Verdana, Arial, Helvetica, sans-serif'

                },

                
     /*   tooltip is what shows on hover   */

                formatter: function (){
					if(this.point.hasOwnProperty("extraData1")){
						var thisPointExtraData1 =this.point.extraData1;	
					}else{
					var thisPointExtraData1 = chart[thisChartIndex].currentExtraData1[this.point.x];
					
					}
					
					if(this.point.hasOwnProperty("extraData2")){
						var thisPointExtraData2 =this.point.extraData2;	
					}else{
					var thisPointExtraData2 = chart[thisChartIndex].currentExtraData2[this.point.x];
					}
					
					
	        var s = '<b>' + this.series.name + '</b><br>' + this.x + " : <b>" + Highcharts.numberFormat(this.y * 1, 1) + '%';
			
			
     /*   console.log(this);   */
			
	 if (this.point.drilldown) {
		 
		 
		s += '<br><span style="font-weight: normal">(Click to drill down)</span>';
	} else {
		if (this.point.level=="top") {
			s = this.point.css("cursor","default"); 
     /*   return false;	   */
		}
		else {
			s += '<br><span style="font-weight: normal">(Click to return to top level)</span>';
		}
	}
	return s;
},

                shared: false

            },

		

		

	

		series: [{

			name: name,

			data: thisDataArray,

			color: 'white'

		}],

		

		

		

		      exporting: {

                buttons: {

                    contextButton: {

                        enabled: true,

                        verticalAlign: 'bottom',

                        x: -10



                    }


                }

            },

					 

		  credits: {

                text:"Source: U.S. Bureau of Labor Statistics.", 
     /*   CREDITS ARE MANDATORY   */

		        position: {align: 'left', x:10}, 
     /*   Higher number moves credits right   */

           style:{

                cursor:"default", color: "black"  
     /*   default or pointer   */

           },

           href:""  
     /*   can add a link here   */

     } 




	}
	
	);
}

