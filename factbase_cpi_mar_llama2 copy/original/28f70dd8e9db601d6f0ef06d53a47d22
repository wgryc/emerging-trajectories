var drillDownOptions = {};

	drillDownOptions.tableDivID = "#tableDiv";
	drillDownOptions.topName = "Major categories";  
	
	drillDownOptions.chartIndex = 0;
	drillDownOptions.selectedColumn = 0;
	
	drillDownOptions.monthHead='';
	setUpDrillDownFromTable(drillDownOptions);
	



});
</script>


