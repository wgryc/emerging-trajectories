        var t = b.createElement(a),
            fs = b.getElementsByTagName(a)[0]; t.async = 1; t.id = e; t.src = s;
        fs.parentNode.insertBefore(t, fs);
    })
        (window, document, 'script', 'https://tag.demandbase.com/527e6d56d1a2150e.min.js', 'demandbase_js_lib');  
</script>


