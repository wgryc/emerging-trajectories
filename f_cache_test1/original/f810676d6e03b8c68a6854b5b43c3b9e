   <section class="default-component  article-content-page article-padded   article-image-no-padding"> 
    <div class="article-hero-container careers-large-hero headline-first"> 
     <div class="article-hero global-grid"> 
      <div class="article-hero-top-row global-grid-col-sm-8 global-grid-col-xl-12"> 
       <div class="article-content-page__series"> 
        <a class="article-content-page__series-text hs_types-body-xl-reg" href="/intelligence/series/articles/index.html">Articles</a> 
       </div> 
       <div class="share-services share-buttons light social-share-bar" data-share-hash=""> 
