<script>
    function pageToolsReplaceTokens() {
      let a = document.getElementsByClassName('pt-item-a');
      const len = a.length;
      const currentUrl = encodeURIComponent(window.location.href);
      const currentUrlToken = "|~current-url~|";
      const pageTitle = encodeURIComponent(document.title);
      const pageTitleToken = "|~page-title~|";
      for(var i=0; i< len; i++) {
        if (a[i].href.includes(currentUrlToken)) {
          updatedHref = a[i].href.replace(currentUrlToken, currentUrl);
          a[i].href = updatedHref;
        }
        if (a[i].href.includes(pageTitleToken)) {
          updatedHref = a[i].href.replace(pageTitleToken, pageTitle);
          a[i].href = updatedHref;
        }
      }
      return false;
    }
    pageToolsReplaceTokens();
</script>

