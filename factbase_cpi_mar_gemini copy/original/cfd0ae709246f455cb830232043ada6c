		<script defer src="https://www.forbes.com/advisor/wp-content/themes/fadv/assets/dist/js/header-search/search-modal.js?ver=1701767720"></script>
	
    <script>
        function lazyLoadBackgroundImage() {
            const observer = new IntersectionObserver((entries, observer) => {
                entries.forEach(entry => {
                    if (entry.isIntersecting) {
                        entry.target.style.backgroundImage = `url(${entry.target.dataset.bgSrc})`;
                        observer.unobserve(entry.target);
                    }
                });
            }, {
                rootMargin: "500px",
            });
            const element = document.querySelectorAll('.lazy-load-bg');
            element.forEach((item) => {
                observer.observe(item);
            })
        }
        document.addEventListener('DOMContentLoaded', () => {
            lazyLoadBackgroundImage();
        })
    </script>
		<script>
			window._staticArticlesUrl = ["https:\/\/www.forbes.com\/advisor\/investing\/best-fidelity-etfs\/","https:\/\/www.forbes.com\/advisor\/investing\/best-charles-schwab-etfs\/","https:\/\/www.forbes.com\/advisor\/investing\/best-sp-500-etfs\/","https:\/\/www.forbes.com\/advisor\/investing\/best-spdr-etfs\/","https:\/\/www.forbes.com\/advisor\/investing\/best-growth-etfs\/","https:\/\/www.forbes.com\/advisor\/investing\/best-value-etfs\/"];
			window._RaFeedData = {"pid":419113,"isReleaseEnvironment":false,"isProductionEnvironment":true,"thumborURL":"https:\/\/thumbor.forbes.com\/thumbor\/"};
			var imgUrl,
				deviceType = getDeviceTypeInNumber(window.deviceType),
				detectMobile = deviceType == 3,
				detectDesktop = deviceType == 1 || deviceType == 2,
				isReleaseEnvironment = window._RaFeedData.isReleaseEnvironment,
				isProductionEnvironment = window._RaFeedData.isProductionEnvironment,
				isThumborSupportedEnv  = isReleaseEnvironment || isProductionEnvironment,
				thumborUrl = window._RaFeedData.thumborURL;
			document.addEventListener("DOMContentLoaded", () => {
				document.querySelectorAll('.ra-feed-img').forEach(element => {
					if (element.getAttribute('src')) {
						let dataImgSrc = element.getAttribute("src");
												if(element.classList.contains("static-ra-image")){
							if (detectMobile && isThumborSupportedEnv) {
								imgUrl = thumborUrl + '660x336/' + dataImgSrc;
							} else if (detectDesktop && isThumborSupportedEnv) {
								imgUrl = thumborUrl + 'fit-in/620x350/' + dataImgSrc;
							} else {
								imgUrl = dataImgSrc;
							}
							element.src = imgUrl;
						} else if(element.classList.contains("static-author-image")){
							if (dataImgSrc.indexOf("https://www.forbes.com/advisor/") > -1 && isThumborSupportedEnv) {
								imgUrl = thumborUrl + '24x24/' + dataImgSrc;
							} else {
								imgUrl = dataImgSrc;
							}
							element.src = imgUrl;
						} else{
							element.src = element.getAttribute('src');
						}
					}
				})
			});
