    (function ($) {
        $(function () {
            $('.article-social-sharing__link').click(function () {
                var url = $(this).data('href');
                window.open(url, 'newwindow', 'width=400,height=600');
            });
            var device_Type = getDeviceTypeInNumber(window.deviceType);
            var isMobile = parseInt(device_Type, 10) === 3;
            if (isMobile) {
                new SimpleBar(document.getElementById('advertiser-disclosure-block'), {
                    autoHide: false
                });
            }
            $(window).scroll(function (e) {
                if (e.target.id !== 'advertiser-disclosure') {
                    if (isMobile) {
                        $('#advertiser-disclosure-block').fadeOut();
                    }
                }
            });

            if(document.querySelector('.cc-compare-widget')){
                var elements = document.getElementsByClassName("cc-compare-widget");
    
                Array.from(elements).forEach(function(element) {
                    if (getDeviceTypeInNumber(window.deviceType) !== 1) {
                        new SimpleBar(element, {
                            autoHide: false
                        });
                    }
                });
            }

            setTimeout(function () {
            creditCardAlignment();
            }, 4000);
            
            $(window).on('resize', window.throttle(windowResizeHandler));
            creditCardAlignment();
        
            function creditCardAlignment() {
                handleCreditCardHeight(document.querySelectorAll(".best-top-bg .best-badge-wrapper", '16px'));
                handleCreditCardHeight(document.querySelectorAll(".fc_item_title"), '8px');
                handleCreditCardHeight(document.querySelectorAll(".partner-offer-text"), '0px');
                handleCreditCardHeight(document.querySelectorAll(".best-top-bg .best-badge-container", '16px'));
                handleCreditCardHeight(document.querySelectorAll(".cc-card .credit-card-wrapper-reward-rate-bottom"), '0');
                handleCreditCardHeight(document.querySelectorAll(".cc-card .cc-stat-group-1"), '0px');
                handleCreditCardHeight(document.querySelectorAll(".cc-card .cc-stat-group-2"), '0px');
                handleCreditCardHeight(document.querySelectorAll(".cc-card .cc-stats-wrapper"), '0px');
                handleCreditCardHeight(document.querySelectorAll(".widget__pdpcompare .cc-card .best-top-bg"), '0');
                handleCreditCardHeight(document.querySelectorAll(".widget__pdpcompare .cc-card"), '16px');
                
            }
            if (document.querySelector('.card-compare') && document.querySelector('.card-compare')[2]) {
                document.getElementsByClassName('card-compare')[2].style.marginRight = "8px";
            }
        
            function applyHeight(cardsList) {
                cardsList.forEach(function (card) {
                    card.style.height = 'auto';
                });
            }
        
            function getMaxHeight(cardsList) {
                let maxHeight = Math.max.apply(null, $(cardsList).map(function () {
                    return $(this).outerHeight();
                }).get());
                return maxHeight;
            }
        
            function handleCreditCardHeight(cardsList, paddingValue) {
                applyHeight(cardsList);
                let maxHeight = getMaxHeight(cardsList);
                cardsList.forEach(function (card) {
                    card.style.height = maxHeight + 'px';
                    card.style.paddingBottom = paddingValue;
                });
            }

            function windowResizeHandler() {
                creditCardAlignment();
            }
        });
    })(jQuery);
    // Remove padding from 'topEdPlaceholder' if #top-0 height is more than 250px
    if ($(".top-ed-placeholder").length) {
        var checkHeightInterval = setInterval(function () {
            var topEdHeight = $("#top-0").height();
            if (topEdHeight > 0) {
                if (topEdHeight > 250) {
                    $(".top-ed-placeholder").addClass("hide-top-ed-padding");
                }
                if ($(".hide-top-ed-padding").length !== 0) {
                    clearInterval(checkHeightInterval);
                }
            }
        }, 500);
    }
    function rightRailBelowRaFeed() {
        var deviceType = getDeviceTypeInNumber(window.deviceType),
            rightRail = $('.right-rail'),
            articlePublishedDate = $('.article_published_date'),
            partnerShortcode = $('.partnerRightRailShortcode');
        if (deviceType === 3 && partnerShortcode.length > 0) {
            partnerShortcode.insertAfter(".feedback-form-wrapper");
        }
        if (deviceType === 3 && rightRail.length && articlePublishedDate.length) {
            rightRail.insertAfter(".article_published_date");
        }
    }

    document.addEventListener("DOMContentLoaded", rightRailBelowRaFeed);
</script>
