                $.ajax({
                    type: "Post",
                    dataType: "html",
                    url: articleUrl,
                    data: { articleIndex: articleIndex, listContents: list },
                    success: function (response) {
                        $("#article").append(response);
                        var listItem = $("[data-article-progress-id=article" + articleIndex + "]");
                        enableLink(listItem);
                        articleIndex++;
                        _inCallback = false;
                        loadGtmInPartialView();
                        if (__sharethis__ && __sharethis__.config) {
                            __sharethis__.init(__sharethis__.config);
                        }
                    },
                    failure: function (response) {
                        console.log(response.responseText);
                    },
                    error: function (response) {
                        console.log(response.responseText);
                    }
                });
            }

            function getArticleViewCount() {
                if (manualWall) {
                    return;
                }

                $.ajax({
                    type: "Get",
                    url:  "/commodityinsights/en/Articles/GetArticleViewCount",
                    success: function (response) {
                        if (response != undefined && response!='') {
                            let ssoCookie = getCookie("beacon");
                            if (ssoCookie === "" && response > 1) {
                                hideArticleContent();
                                enableLoginWall();
                            }
                        }
                    },
                    failure: function (response) {
                        console.log(response.responseText);
                    },
                    error: function (response) {
                        console.log(response.responseText);
                    }
                });
            }

            function gtmOverlayConfirmationPush() {
                window.dataLayer.push({
                    'event': 'registrationPopup'
                });
            }

            function enableLoginWall() {
                loginWall = true;
                $("#gated-overlay").show();
                gtmOverlayConfirmationPush();
            }

            function hideArticleContent() {
                $(".article__wrapper .article__content").addClass('gated');
            }

            function switchToManual() {
                // reset all
                loginWall = false;
                manualWall = true;
                _inCallback = false;

                $("#gated-overlay").hide();
                $(".article__wrapper .article__content").removeClass('gated');
            }

            window.switchToManual = switchToManual;
            window.enableLoginWall = enableLoginWall;
        });
    })();
</script>

<script type="text/javascript">
    $('.mp3download').on("click", function () {
        var host = window.location.host;
        var protocol = window.location.protocol;
        var formUrl = "";
        var href = window.location.href;
        if ($(this).attr('url') != "") {
            formUrl = protocol + "//" + host + $(this).attr('url');
        }
        else {
            formUrl = protocol + "//" + host + href.replace(protocol + "//" + host).substr(0, href.lastIndexOf('/')) + "/mp3" + href.replace(protocol + "//" + host).substr(href.lastIndexOf("/"), (href.length - href.lastIndexOf("/"))) + ".mp3";
        }

        window.open(formUrl, '_blank').focus();

    });

    $(document).ready(function () {
        var collection = $(".article_inlist");

        $.each(collection, function (idx, val) {
            disableLink($(this));
        });
        enableLink($('[data-article-progress-id="article0"]'));
    });
    
    function disableLink(link) {
        // 1. Add isDisabled class to parent span
        link.addClass('isDisabled');
        // 3. Remove href
        link.href = '';
    }

    function enableLink(link) {
        // 1. Remove 'isDisabled' class from parent span
        link.removeClass('isDisabled');
        // 2. Set href
        link.href = "#" + link.attr('data-article-progress-id');
    }
</script>


