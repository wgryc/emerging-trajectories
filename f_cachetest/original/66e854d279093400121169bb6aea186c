    <script type="text/javascript">
        $(document).ready(function () {

            // added script for user story 3521791 start
            var hide = true;
            $('body').on("click", function () {
                if (hide) jQuery('.share-dropdown').removeClass('dropdown-show');
                hide = true;
            });

            // add and remove .active
            $('body').on('click', '.share-dropdown-icon', function () {

                var self = jQuery(this).parent();

                if (self.hasClass('dropdown-show')) {
                    jQuery('.share-dropdown-icon').parent().removeClass('dropdown-show');
                    return false;
                }

                jQuery('.share-dropdown-icon').parent().removeClass('dropdown-show');

                self.toggleClass('dropdown-show');
                hide = false;
            });

            // added script for user story 3521791 end
            $(".btn-facebook").click(function () {
                var data_url = jQuery(this).attr("data-url");
                var data_title = jQuery(this).attr("data-title");

                window.open('https://www.facebook.com/sharer/sharer.php?u=' + data_url + '&t=' + data_title, '',
                    'menubar=no,toolbar=no,resizable=yes,scrollbars=yes,height=300,width=600');
            });

            function twitterShare() {
                window.twttr = (function (d, s, id) {
                    var js, fjs = d.getElementsByTagName(s)[0],
                        t = window.twttr || {};
                    if (d.getElementById(id)) return t;
                    js = d.createElement(s);
                    js.id = id;
                    js.src = "https://platform.twitter.com/widgets.js";
                    fjs.parentNode.insertBefore(js, fjs);



                    t._e = [];
                    t.ready = function (f) {
                        t._e.push(f);
                    };



                    return t;
                }(document, "script", "twitter-wjs"));
            }

            function linkedinShare() {
                window.widget = (function (d, s, id) {
                    var js, fjs = d.getElementsByTagName(s)[0],
                        t = window.twttr || {};
                    if (d.getElementById(id)) return t;
                    js = d.createElement(s);
                    js.id = id;
                    js.src = "https://platform.linkedin.com/in.js";
                    fjs.parentNode.insertBefore(js, fjs);



                    t._e = [];
                    t.ready = function (f) {
                        t._e.push(f);
                    };



                    return t;
                }(document, "script", "widget"));
            }

            twitterShare();

            linkedinShare();

            const openContactMdl = $('a[href="#contactusform"]');
            const closeModal = $('.contactus-modal .contact-close');
            openContactMdl.on('click', function () {
                $('.contactus-modal').css('display', 'block');
            });
            closeModal.on('click', function () {
                $('.contactus-modal').css('display', 'none');
            });
        });



    </script>





