<script type="text/javascript">

    function handleBookmarkClick2862056(e) {
        let postAction = null;
        if ($(e).hasClass('bookmarked')) {
            postAction = "delete";
        } else if ($(e).hasClass('bookmark')) {
            postAction = "save";
        } else {
            return;
        }

        var bookmark = {
            BranchId: '2862056', ContentUrl: window.location.pathname,
            PublishedDate: '10/11/2023 5:27:00 PM', Region:'Americas,EMEA', ContentType: 'News', Title:'Oil, gasoline prices set to rise in 2024 as OPEC+ production cuts take their toll: EIA'
        };

        postBookmarkDetails(bookmark, postAction, function (data) {
            if (data.isSuccess) {
                $(e).toggleClass('bookmarked', postAction == "save");
            } else if (data.isLimitExceeded) {
                let exceedlink = document.getElementById('bookmark_exceedlink');
                exceedlink.click();
                // Show Popup
            } else {
                // Show / log error / revert UI change if any
            }
        });
    }
</script>

