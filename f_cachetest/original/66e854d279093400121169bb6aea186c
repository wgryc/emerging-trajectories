                addSecondArticleTitle();
                enableLoginWall();
            } else if (ssoCookie === "" && articleHitCookieValue > 1) {
                hideArticleContent();
                enableLoginWall();
            }

            function getWindowHeight() {
                return 'innerHeight' in window ? window.innerHeight : document.documentElement.offsetHeight;
            }

            function addSecondArticleTitle()
            {
                var secondarticleTitle = $('.article__list-item-content p').length >=3 ? $('.article__list-item-content p')[2].innerText : '';
                var secondArticleCommodity = $('.article__list-item-content h8').length >=3 ? $('.article__list-item-content h8')[2].innerText : '';
