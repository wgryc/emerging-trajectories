            }

            //Check Element is visible on the screen
            function isElementInViewport(el) {
                var rect = el[0].getBoundingClientRect();
                var top = rect.top;
                var bottom = rect.bottom;
                var pixelOffset = 10;

                var isTopInView = top + pixelOffset < windowsHeight && top > 0;
                var isBottomInView = bottom > pixelOffset && bottom < windowsHeight;

                return isTopInView || isBottomInView;
            }

            // callback
            function onScroll() {
                if ($(window).scrollTop() >= ($(window).scrollTop() + $(window).height()) / 2) {
                    if (articleIndex < 7 && !_inCallback) {
                        _inCallback = true;
                        t0 = performance.now();
                        getArticle();
                        t1 = performance.now();
                        console.log("Took: " + (t1 - t0) + "msecs");
                    }
                }
                if (ssoCookie === "" && $('.dontshow').length > 0 && isElementInViewport($('.dontshow'))) {
                    getArticleViewCount();
                }
            }

