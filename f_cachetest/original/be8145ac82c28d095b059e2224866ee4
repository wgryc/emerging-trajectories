                    var darkImgs = document.querySelectorAll("img.dark-image");
                    var lightImgs = document.querySelectorAll("img.light-image");
                    var rootid = document.querySelector(".gs-header");
                    var clientLogin = document.querySelector(".client-login-button-text");
                    var icon = document.querySelector(".ui-icon-btn");
                    var searchIcon = document.querySelector(".ui-icon-btn--search");
                    var myDivObjBgColor = window.getComputedStyle(rootid).backgroundColor;
                    if(myDivObjBgColor == "rgb(255, 255, 255)") {
                        lightImgs.forEach(img => {
                            img.style.display = 'none';
                        })
                    } else {
                        darkImgs.forEach(img => {
                            img.style.display = 'none';
                        })
                        clientLogin.style.color = 'rgb(255, 255, 255)';
                        icon.style.color = 'rgb(255, 255, 255)';
                        searchIcon.style.color = 'rgb(255, 255, 255)';
