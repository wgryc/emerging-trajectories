<script>
    (function() {
        //console.log("SSConsent Setup");
        var ssintv1=setInterval( function() {
            if(window.evidon && window.evidon.notice){
                const ssevidonf_optout = function() { 
                    var minutes = 525600, 
                    date = new Date(); 
                    date.setTime(date.getTime() + (minutes * 60 * 1000)); 
                    document.cookie = "ssDisabled=1; expires=" + date.toUTCString() + "; path=/; Secure; SameSite=Lax;"; 
                    fetch("https://www.fitchratings.com/__ssobj/opt-out");
                }
                window.evidon.closeCallback = function() {
                    console.log("In closeCallback");
                    ssevidonf_optout();
                };
                window.evidon.consentWithdrawnCallback = function() {
                    console.log("In consentWithdrawnCallback");
                    ssevidonf_optout();
                };
                window.evidon.consentDeclinedCallback = function() {
                    console.log("In consentDeclinedCallback");
                    ssevidonf_optout();
                };
                window.evidon.priorConsentCallback = function(categories, vendors, cookies) {
                    console.log("In priorConsentCallback: ", categories);
                    if (categories && !categories.all && !categories.performance) {
                        ssevidonf_optout();
                    }
                }
                clearInterval(ssintv1);
            }
        }, 500);
    })();
</script>
<script>
window.SS && SS.Require(function() {
  (function(history){
    var pushState = history.pushState;
      history.pushState = function(state, key, path) {
      if (typeof history.onpushstate == "function") {
        history.onpushstate({state: state, path: path})
      }
      pushState.apply(history, arguments)
    }
    
    window.onpopstate = history.onpushstate = function(e) {
      trackPath(e); 
    }

    trackPath();

  })(window.history);

  function trackPath(e) {
    let path;
    if (e && e.path) {
      path = e.path;
    } else {
      path = window.location.pathname;
    }
    window.SS.EventTrack.rp("ssupu-page_view", path);
  }
});
</script>

