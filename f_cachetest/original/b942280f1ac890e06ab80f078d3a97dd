<script  type="text/javascript" data-sitespect="true">
window.__sscf.cf_5453  = function() {
let targetBtns = Array.prototype.slice.call(this.querySelectorAll(".ss-nav-btn"));
let targetBtnExists = targetBtns.reduce(function(acc, el) {
    if (el.innerText.toLowerCase() === "insights") {
        el.classList.remove("ss-nav-btn");
    }
    return acc || el.innerText.toLowerCase() === "sectors";
}, false);

if (!targetBtnExists) {
    setupNavListeners(this);
    setupInnerNavListeners();
    createNavButton(this);
}

if (!document.querySelector(".ss-nav-modal")) {
    window._ssCreateNewModal();
}

function setupInnerNavListeners() {
    document.addEventListener("click", function(e) {
        if (e.target.matches("span[data-testid='SubNavigationTitle']")) {
            if (e.target.innerText.toLowerCase() === "sectors") {
                if (!window._ssNav || !_ssNav.clicked) {
                    window.SS.EventTrack.rp("modal_sectors_click");
                }
            } else if (e.target.innerText.toLowerCase() === "topics") {
                if (!window._ssNav || !_ssNav.clicked) {
                    window.SS.EventTrack.rp("modal_topics_click");
                }
            }
        } else if (e.target.closest(".ss-nav-modal")) {
            closeNewModal();
        }
    });
}

function setupNavListeners(parent) {
    // close the new modal when other nav buttons are clicked
    let navBtns = Array.prototype.slice.call(parent.querySelectorAll("li"));
    navBtns.forEach(function(btn) {
        if (!btn._ssListener)
            btn.addEventListener("click", function(e) {
                if (e.target.classList.contains("ss-nav-btn") || e.target.querySelector(".ss-nav-btn")) {
                    return;
                }
                let overlay = document.querySelector(".ss-bg-overlay");
                if (overlay && overlay.classList.contains("active")) {
                    closeNewModal();
                }
            });
        btn._ssListener = true;
    });
}
// create and insert the new nav button along with its click handler
function createNavButton(parent) {
    let ref = parent.querySelector("li:nth-of-type(2)");
    let newBtnWrap = '<li class="navigation__group"><span class="overlay overlay__navigation ss-nav-btn" role="button" tabindex="0">Sectors</span></li>';
    ref.insertAdjacentHTML("afterend", newBtnWrap);
    let btn = document.querySelector(".ss-nav-btn");
    btn.addEventListener("click", function(e) {
        if (e.target.innerText.toLowerCase() === "insights") { 
            return; 
        }

        SS.EventTrack.rp("new-top-nav-btn-click");

        if (e.target.classList.contains("active")) {
            closeNewModal();
        } else {
            adjustCurrentNavState(parent);
            adjustCurrentAppState();
            createNavCloseButton();
            showModal();
        }
    });
}


function showModal() {
    let newModal = document.querySelector(".ss-nav-modal");
    newModal.classList.add("navigation__overlay", "overlay-content-wrapper");
    newModal.style.display = "block";
}
function hideModal() {
    let newModal = document.querySelector(".ss-nav-modal");
    newModal.style.display = "none";
    newModal.classList.remove("navigation__overlay", "overlay-content-wrapper");
}

// create and insert the new close modal button
function createNavCloseButton() {
    let navRef = document.querySelector(".region.region--nav")
    let newCloseBtn = '<span class="navigation__overlay-close" role="button" tabindex="0" data-testid="handleClose"><i class="fas fa-times"></i></span>';
    navRef.insertAdjacentHTML("afterend", newCloseBtn);
    let closeBtn = document.querySelector(".navigation__overlay-close");
    closeBtn.addEventListener("click", closeNewModal);
}

// adjust app component classes to the correct state for the modal/overlay to display
function adjustCurrentAppState() {
    let overlay = document.querySelector(".ss-bg-overlay");
    let nav = document.querySelector(".navigation__main");
    let menuItem = document.querySelector(".js-nav-toggle");
    let header = document.querySelector(".hasHeader");
    let altHeader = document.querySelector(".header--overlay");
    let actionBar = document.querySelector(".action-bar");
    let search = document.querySelector("#gatsby-focus-wrapper > div > div");
    let btn = document.querySelector(".ss-nav-btn");
    
    if (header) {
        btn && btn.classList.add("active");
        search && search.classList.add("active--search");
        actionBar && actionBar.classList.remove("active--action-bar");
        overlay && overlay.classList.add("active");
        nav && nav.classList.add("active");
        menuItem && menuItem.classList.add("active");
        header && header.classList.add("header--inverted");
        header && header.classList.remove("active--action-bar", "header--overlay");
    } else if (altHeader) {
        let search = document.querySelector(".header--overlay > div");
        altHeader.classList.add("header--inverted");
        altHeader.classList.remove("header--overlay");
        btn && btn.classList.add("active");
        search && search.classList.add("active--search");
        actionBar && actionBar.classList.remove("active--action-bar");
        overlay && overlay.classList.add("active");
        nav && nav.classList.add("active");
        menuItem && menuItem.classList.add("active");
    }
}

// check for an already active nav modal and deactivate (by clicking on it's nav button)
function adjustCurrentNavState(parent) {
    let existingModal = document.querySelector(".navigation__overlay.overlay-content-wrapper");
    if (existingModal) {
        let navBtns = Array.prototype.slice.call(parent.querySelectorAll("li span"));
        navBtns.forEach(function(btn) {
            if (btn.classList.contains("active")) {
                btn.click();
            }
        });
    }    
}

function closeNewModal() {
    let btn = document.querySelector(".ss-nav-btn");
    let closeBtn = document.querySelector(".navigation__overlay-close");
    let overlay = document.querySelector(".ss-bg-overlay");
    let nav = document.querySelector(".navigation__main");
    let menuItem = document.querySelector(".js-nav-toggle");
    let header = document.querySelector(".hasHeader");
    let altHeader = document.querySelector(".header--inverted");
    let actionBar = document.querySelector(".action-bar");
    let search = document.querySelector("#gatsby-focus-wrapper > div > div");
    if (overlay && overlay.classList.contains("active")) {
        if (header) {
            btn && btn.classList.remove("active");
            hideModal();
            closeBtn && closeBtn.parentElement.removeChild(closeBtn);
            search && search.classList.remove("active--search");
            actionBar && actionBar.classList.add("active--action-bar");
            overlay && overlay.classList.remove("active");
            nav && nav.classList.remove("active");
            menuItem.classList.remove("active");
            menuItem && header.classList.remove("header--inverted");
            header && header.classList.add("active--action-bar", "header--overlay");
            hideModal();
        } else if (altHeader) {
            let search = document.querySelector(".header--inverted > div");
            altHeader.classList.remove("header--inverted");
            altHeader.classList.add("header--overlay");
            btn && btn.classList.remove("active");
            hideModal();
            search && search.classList.remove("active--search");
            actionBar && actionBar.classList.add("active--action-bar");
            overlay && overlay.classList.remove("active");
            nav && nav.classList.remove("active");
            menuItem && menuItem.classList.remove("active");
        }
    }
}
}
</script>
